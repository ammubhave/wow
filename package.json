{
  "name": "wow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --restart-tries -1 \"vite --host 0.0.0.0 --port 8788\" \"mkdir -p dist && wrangler dev --ip 0.0.0.0\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cf-typegen": "npx wrangler types --x-include-runtime runtime.d.ts",
    "postinstall": "zenstack generate",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\"",
    "deploy": "wrangler deploy",
    "db:push:remote": "prisma migrate diff --from-url file:./dev.sqlite --to-schema-datamodel prisma/schema.prisma --script | turso db shell wow-dev",
    "db:push:remote:prod": "prisma migrate diff --from-url file:./dev.sqlite --to-schema-datamodel prisma/schema.prisma --script | turso db shell wow",
    "db:push:local": "prisma db push"
  },
