{
  "name": "wow",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm decrypt && vite dev --port 3000",
    "build": "CLOUDFLARE_ENV=production vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "deploy": "wrangler deploy --env production",
    "format": "oxfmt",
    "lint": "oxlint --type-check --type-aware",
    "postinstall": "concurrently 'wrangler types --strict-vars=false' 'touch src/lib/db/auth-schema.ts && pnpm dlx @better-auth/cli@latest generate --config src/lib/auth.ts --output src/lib/db/auth-schema.ts --yes'",
    "prepare": "husky",
    "decrypt": "dotenvx decrypt --env-file .env.local.encrypted --stdout | node -e \"process.stdout.write(require('fs').readFileSync(0,'utf8').slice(0,-1))\" > .env.local",
    "encrypt": "dotenvx encrypt --env-file .env.local --stdout | node -e \"process.stdout.write(require('fs').readFileSync(0,'utf8').slice(0,-1))\" > .env.local.encrypted",
    "db:push": "mkdir -p .wrangler/state/v3/d1/miniflare-D1DatabaseObject/ && drizzle-kit push"
  },
